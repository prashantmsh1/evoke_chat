        "@types/bcryptjs": "^2.4.6",
        "@types/jsonwebtoken": "^9.0.10",
        "bcryptjs": "^3.0.2",
        "compression": "^1.8.0",
        "cors": "^2.8.5",
        "dotenv": "^16.5.0",
        "express": "^5.1.0",
        "express-rate-limit": "^7.5.0",
        "helmet": "^8.1.0",
        "jsonwebtoken": "^9.0.2",
